{
    "name": "osu! preview",
    "version": "0.1.0",
    "description": "Preview osu! beatmaps inside the browser",
    "permissions": ["storage", "tabs", "webRequest", "declarativeNetRequest", "unlimitedStorage", "downloads"],
    "host_permissions": ["https://*.ppy.sh/*"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://osu.ppy.sh/*"],
        "js": ["contentScript.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": [
        ],
        "matches": ["https://*.youtube.com/*"]
      }
    ],
    "action": {
      "default_title": "osu! preview",
      "default_popup": "popup.html"
    },
    "manifest_version": 3
}